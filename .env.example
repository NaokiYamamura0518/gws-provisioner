# ===== Basic認証 =====
BASIC_AUTH_USER=admin
BASIC_AUTH_PASS=changeme

# ===== Google Workspace =====
# Workload Identity Federation のオーディエンス URI
# 形式: //iam.googleapis.com/projects/PROJECT_NUMBER/locations/global/workloadIdentityPools/POOL_ID/providers/PROVIDER_ID
GOOGLE_WORKLOAD_IDENTITY_AUDIENCE=//iam.googleapis.com/projects/123456789/locations/global/workloadIdentityPools/vercel-pool/providers/vercel-provider

# サービスアカウントのメールアドレス（秘密鍵は不要）
GOOGLE_SERVICE_ACCOUNT_EMAIL=gws-provisioner@your-project.iam.gserviceaccount.com

# ドメインワイド委任で偽装する管理者メールアドレス
GOOGLE_ADMIN_EMAIL=admin@example.com

# Google Workspaceのドメイン（メールアドレス自動生成に使用）
GOOGLE_DOMAIN=example.com

# 新規アカウントの初期パスワード
INITIAL_PASSWORD=Welcome123!

# ===== Slack =====
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/XXXXX/YYYYY/ZZZZZ
